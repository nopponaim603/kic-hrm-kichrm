<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:VerticalPanel width="100%" height="">
			<g:Label text="Daily Report" width="100%"/>
			<g:Button width="100%" ui:field="gBack">Back</g:Button>
			<g:HorizontalPanel width="100%">
				<g:Button text="Previous" width="100%" ui:field="btnPrevious"/>
				<g:Button width="100%" text="Today" ui:field="btnToday"/>
				<g:Button width="100%" text="Next" ui:field="btnNext"/>
			</g:HorizontalPanel>
			<g:Grid width="100%" ui:field="gGrid">
				<g:row>
					<g:customCell>
						<g:Label text="Employee ID" width="79px"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="State" width="50px"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Time" width="109px"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Address" width="194px"/>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 